{
  "name": "adonis-ts",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --watch",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "queue:start": "node ace queue:listen",
    "queue:dashboard": "node ace queue:dashboard",
    "openapi:spec": "tsoa spec",
    "openapi:build": "redocly build-docs openapi/build/swagger.yaml --output openapi/build/index.html",
    "typechain:gen": "typechain --target ethers-v6 --out-dir config/ './config/fusion-abi.json'"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#jobs/*": "./app/jobs/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#serializers/*": "./app/serializers/*.js",
    "#services/*": "./app/services/*.js", 
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#start/*": "./start/*.js",
    "#tests/*": "./tests/*.js",
    "#config/*": "./config/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.2.3",
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.3.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@japa/api-client": "^2.0.2",
    "@japa/assert": "^2.1.0",
    "@japa/expect": "^3.0.1",
    "@japa/plugin-adonisjs": "^3.0.0",
    "@japa/runner": "^3.1.1",
    "@redocly/cli": "^1.10.3",
    "@swc/core": "^1.4.7",
    "@typechain/ethers-v6": "^0.5.1",
    "@types/lodash": "^4.17.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.27",
    "eslint": "^8.57.0",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsoa": "^6.1.4",
    "typechain": "^8.3.2",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.1.1",
    "@adonisjs/bouncer": "^3.1.1",
    "@adonisjs/core": "^6.3.1",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/lucid": "^20.4.0",
    "@adonisjs/redis": "^8.0.1",
    "@its/sdk": "file:lib/sdk",
    "@vinejs/vine": "^1.8.0",
    "ethers": "^6.12.1",
    "fs": "0.0.1-security",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "parse-duration": "^1.1.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "web3": "^4.8.0"
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/app"
  },
  "prettier": "@adonisjs/prettier-config",
  "dependenciesMeta": {
    "@its/sdk": {
      "injected": true
    }
  }
}
